/**index.wxss**/
.container{
  overflow: scroll;
}
.banner{
  display: block;
  width: 100%;
  min-height: 800rpx;
  background: #ccc;
}
.banner-item .banner-item-img{
  display: block;
  width: 100%;
  height: auto;
}
.cus{
  width: 500rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx 0;
}
.cus image{
  width: 200rpx;
  height: 200rpx;
}
.cus .cusimg{
  width: 150rpx;
  height: 50rpx;
}
